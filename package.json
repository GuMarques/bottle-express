{
  "name": "bottle-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "install:dev": "docker build -t bottle-express:dev .",
    "start:dev": "docker run -it --rm -v ${PWD}:/app -v /app/node_modules -p 8080:8080 -e CHOKIDAR_USEPOLLING=true bottle-express:dev",
    "start": "nodemon ./bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mustache-express": "^1.3.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
